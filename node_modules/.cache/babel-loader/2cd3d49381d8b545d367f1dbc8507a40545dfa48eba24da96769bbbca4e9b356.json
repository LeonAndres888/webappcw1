{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ParentComponent from \"./components/ParentComponent.vue\";\nimport LessonComponent from \"./components/LessonComponent.vue\";\nimport CheckoutComponent from \"./components/CheckoutComponent.vue\";\nexport default {\n  name: \"App\",\n  components: {\n    ParentComponent,\n    LessonComponent,\n    CheckoutComponent\n  },\n  data() {\n    return {\n      sitename: \"👨‍🎓 STUDY SESSION STORE 👩‍🎓\",\n      showProduct: true,\n      products: [],\n      cart: [],\n      searchLesson: \"\",\n      sortAttribute: \"title\",\n      sortOrder: \"ascending\"\n    };\n  },\n  computed: {\n    cartItemCount() {\n      // Calculates the total count of items in the cart\n      return this.cart.reduce((total, item) => total + item.quantity, 0);\n    },\n    sortedProducts() {\n      // Returns the products sorted based on the selected attribute and order\n      let sorted = [...this.products].sort((a, b) => {\n        let comparison = 0;\n        if (a[this.sortAttribute] < b[this.sortAttribute]) {\n          comparison = -1;\n        } else if (a[this.sortAttribute] > b[this.sortAttribute]) {\n          comparison = 1;\n        }\n        return this.sortOrder === \"ascending\" ? comparison : comparison * -1;\n      });\n      return sorted;\n    }\n  },\n  methods: {\n    toggleCart() {\n      // Toggles between showing the product list and the cart\n      this.showProduct = !this.showProduct;\n    },\n    addItemToCart(product) {\n      // Adds an item to the cart or increases its quantity if it's already in the cart\n      let found = this.cart.find(item => item.id === product.id);\n      if (found) {\n        found.quantity += 1;\n      } else {\n        this.cart.push({\n          ...product,\n          quantity: 1\n        });\n      }\n    },\n    removeItemFromCart(item) {\n      // Removes an item from the cart\n      const index = this.cart.findIndex(cartItem => cartItem.id === item.id);\n      if (index !== -1) {\n        this.cart.splice(index, 1);\n      }\n    },\n    fetchLessons() {\n      // Fetches lesson data from the server\n      fetch(\"https://storefinal-env.eba-vfsgptpf.us-east-1.elasticbeanstalk.com/api/lessons\").then(response => {\n        if (!response.ok) throw new Error(\"Network response was not ok\");\n        return response.json();\n      }).then(data => {\n        this.products = data;\n      }).catch(error => {\n        console.error(\"Error fetching lessons:\", error);\n      });\n    }\n  },\n  mounted() {\n    // Fetches lessons when the component mounts\n    this.fetchLessons();\n  }\n};","map":{"version":3,"names":["ParentComponent","LessonComponent","CheckoutComponent","name","components","data","sitename","showProduct","products","cart","searchLesson","sortAttribute","sortOrder","computed","cartItemCount","reduce","total","item","quantity","sortedProducts","sorted","sort","a","b","comparison","methods","toggleCart","addItemToCart","product","found","find","id","push","removeItemFromCart","index","findIndex","cartItem","splice","fetchLessons","fetch","then","response","ok","Error","json","catch","error","console","mounted"],"sources":["C:\\Users\\andre\\OneDrive\\Desktop\\CW2FinalFolder\\webappcw1\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <header-component\n      :sitename=\"sitename\"\n      :cartItemCount=\"cartItemCount\"\n      @toggle-cart=\"toggleCart\"\n    />\n    <lesson-component\n      v-if=\"showProduct\"\n      :lessons=\"sortedProducts\"\n      @add-to-cart=\"addItemToCart\"\n    />\n    <checkout-component\n      v-else\n      :cart=\"cart\"\n      @remove-from-cart=\"removeItemFromCart\"\n    />\n  </div>\n</template>\n\n<script>\nimport ParentComponent from \"./components/ParentComponent.vue\";\nimport LessonComponent from \"./components/LessonComponent.vue\";\nimport CheckoutComponent from \"./components/CheckoutComponent.vue\";\n\nexport default {\n  name: \"App\",\n  components: {\n    ParentComponent,\n    LessonComponent,\n    CheckoutComponent,\n  },\n  data() {\n    return {\n      sitename: \"👨‍🎓 STUDY SESSION STORE 👩‍🎓\",\n      showProduct: true,\n      products: [],\n      cart: [],\n      searchLesson: \"\",\n      sortAttribute: \"title\",\n      sortOrder: \"ascending\",\n    };\n  },\n  computed: {\n    cartItemCount() {\n      // Calculates the total count of items in the cart\n      return this.cart.reduce((total, item) => total + item.quantity, 0);\n    },\n    sortedProducts() {\n      // Returns the products sorted based on the selected attribute and order\n      let sorted = [...this.products].sort((a, b) => {\n        let comparison = 0;\n        if (a[this.sortAttribute] < b[this.sortAttribute]) {\n          comparison = -1;\n        } else if (a[this.sortAttribute] > b[this.sortAttribute]) {\n          comparison = 1;\n        }\n        return this.sortOrder === \"ascending\" ? comparison : comparison * -1;\n      });\n      return sorted;\n    },\n  },\n  methods: {\n    toggleCart() {\n      // Toggles between showing the product list and the cart\n      this.showProduct = !this.showProduct;\n    },\n    addItemToCart(product) {\n      // Adds an item to the cart or increases its quantity if it's already in the cart\n      let found = this.cart.find((item) => item.id === product.id);\n      if (found) {\n        found.quantity += 1;\n      } else {\n        this.cart.push({ ...product, quantity: 1 });\n      }\n    },\n    removeItemFromCart(item) {\n      // Removes an item from the cart\n      const index = this.cart.findIndex((cartItem) => cartItem.id === item.id);\n      if (index !== -1) {\n        this.cart.splice(index, 1);\n      }\n    },\n    fetchLessons() {\n      // Fetches lesson data from the server\n      fetch(\n        \"https://storefinal-env.eba-vfsgptpf.us-east-1.elasticbeanstalk.com/api/lessons\"\n      )\n        .then((response) => {\n          if (!response.ok) throw new Error(\"Network response was not ok\");\n          return response.json();\n        })\n        .then((data) => {\n          this.products = data;\n        })\n        .catch((error) => {\n          console.error(\"Error fetching lessons:\", error);\n        });\n    },\n  },\n  mounted() {\n    // Fetches lessons when the component mounts\n    this.fetchLessons();\n  },\n};\n</script>\n\n<style>\n/* Global styles */\n</style>\n"],"mappings":";AAqBA,OAAOA,eAAc,MAAO,kCAAkC;AAC9D,OAAOC,eAAc,MAAO,kCAAkC;AAC9D,OAAOC,iBAAgB,MAAO,oCAAoC;AAElE,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVJ,eAAe;IACfC,eAAe;IACfC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,iCAAiC;MAC3CC,WAAW,EAAE,IAAI;MACjBC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,EAAE;MACRC,YAAY,EAAE,EAAE;MAChBC,aAAa,EAAE,OAAO;MACtBC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd;MACA,OAAO,IAAI,CAACL,IAAI,CAACM,MAAM,CAAC,CAACC,KAAK,EAAEC,IAAI,KAAKD,KAAI,GAAIC,IAAI,CAACC,QAAQ,EAAE,CAAC,CAAC;IACpE,CAAC;IACDC,cAAcA,CAAA,EAAG;MACf;MACA,IAAIC,MAAK,GAAI,CAAC,GAAG,IAAI,CAACZ,QAAQ,CAAC,CAACa,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;QAC7C,IAAIC,UAAS,GAAI,CAAC;QAClB,IAAIF,CAAC,CAAC,IAAI,CAACX,aAAa,IAAIY,CAAC,CAAC,IAAI,CAACZ,aAAa,CAAC,EAAE;UACjDa,UAAS,GAAI,CAAC,CAAC;QACjB,OAAO,IAAIF,CAAC,CAAC,IAAI,CAACX,aAAa,IAAIY,CAAC,CAAC,IAAI,CAACZ,aAAa,CAAC,EAAE;UACxDa,UAAS,GAAI,CAAC;QAChB;QACA,OAAO,IAAI,CAACZ,SAAQ,KAAM,WAAU,GAAIY,UAAS,GAAIA,UAAS,GAAI,CAAC,CAAC;MACtE,CAAC,CAAC;MACF,OAAOJ,MAAM;IACf;EACF,CAAC;EACDK,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX;MACA,IAAI,CAACnB,WAAU,GAAI,CAAC,IAAI,CAACA,WAAW;IACtC,CAAC;IACDoB,aAAaA,CAACC,OAAO,EAAE;MACrB;MACA,IAAIC,KAAI,GAAI,IAAI,CAACpB,IAAI,CAACqB,IAAI,CAAEb,IAAI,IAAKA,IAAI,CAACc,EAAC,KAAMH,OAAO,CAACG,EAAE,CAAC;MAC5D,IAAIF,KAAK,EAAE;QACTA,KAAK,CAACX,QAAO,IAAK,CAAC;MACrB,OAAO;QACL,IAAI,CAACT,IAAI,CAACuB,IAAI,CAAC;UAAE,GAAGJ,OAAO;UAAEV,QAAQ,EAAE;QAAE,CAAC,CAAC;MAC7C;IACF,CAAC;IACDe,kBAAkBA,CAAChB,IAAI,EAAE;MACvB;MACA,MAAMiB,KAAI,GAAI,IAAI,CAACzB,IAAI,CAAC0B,SAAS,CAAEC,QAAQ,IAAKA,QAAQ,CAACL,EAAC,KAAMd,IAA